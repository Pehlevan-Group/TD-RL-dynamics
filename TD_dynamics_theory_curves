    "get_coords = lambda seed: lax.scan(update_fn, [[box_len//2,box_len//2], random.PRNGKey(seed)], None, length = 100)[1]\n",
    "all_coords = vmap(get_coords)(jnp.arange(num_samples))\n"
   ]
