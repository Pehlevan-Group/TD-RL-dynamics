    "plt.loglog(losses_th, '--', color = 'black', label = 'Theory')\n",
    "plt.xlabel(r'Steps', fontsize = 20)\n",
    "plt.ylabel(f'Value Est. Error',fontsize = 20)\n",
    "plt.tight_layout()\n",
    "\n",
    "plt.show()"
   ]
