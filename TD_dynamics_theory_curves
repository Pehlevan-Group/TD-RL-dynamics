   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "batches = [1,5,10,25, 50]\n",
    "all_all_losses = [ expt_learn_curves(Sigma_diag, sqrt_Sigma, wR, wV, eta, gamma, batch, num_rep = 20).mean(axis=0) for batch in batches]\n",
    "all_all_losses_th = [ compute_theory(Sigma, Sigma_diag, Sigma_shift_diag, wR, wV, eta, gamma,batch) for batch in batches ] "
   ]
  },
